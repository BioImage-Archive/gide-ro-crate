    {
      "@id": "#75548155-9775-454e-9fee-3977b9db24a8",
      "@type": [
        "BioSample"
      ],
      "description": "HeLa cells expressing Mito-EGFP and genetically encoded multimeric particle (GEM) GEM2 system. Mito-EGFP specifically labels mitochondria, while the GEM2 system provides genetically encoded multimeric nanoparticles for visualization by electron microscopy.",
      "name": "_:HeLa cells expressing Mito-EGFP with GEM2 labelling",
      "taxonomicRange": {
        "@id": "obo:NCBITaxon_9606"
      }
    },
    {
      "@id": "#646ea0bc-9642-42a3-861b-8f89f43f47dd",
      "@type": [
        "LabProtocol"
      ],
      "description": "Cryo-TEM montages and tilt series were collected on a Titan Krios G3 (Thermo Fisher Scientific) equipped with a Gatan K2 Summit detector and Quantum energy filter, or a Gatan K3 detector and BioQuantum energy filter, using SerialEM. Grids were loaded such that the lamella pretilt axis aligns with the microscope stage tilt axis. Images were acquired at a pixel size of 3.370 or 3.425 Å/pixel at 1.5–4.0 µm defocus, with an electron dose of 2.0–2.5 e−/Å2 per image fractionated over 8–10 frames. A dose-symmetric tilt scheme was used with 2° increments starting from the lamella pretilt (±13°) and an effective tilt range of +56° to −56° using SerialEM. Data were collected with a Volta phase plate and 20 eV slit width.",
      "labEquipment": "Titan Krios G3 (Thermo Fisher Scientific) with Gatan K2 or K3 detector",
      "measurementTechnique": [
        {
          "@id": "obo:FBbi_00000256"
        }
      ],
      "name": "Cryo-electron tomography with Volta phase plate"
    },
    {
      "@id": "#d7abef33-0b94-4957-9ac7-d8062ff51993",
      "@type": [
        "LabProtocol"
      ],
      "description": "Cryo-TEM montages and tilt series were collected on a Titan Krios G3 (Thermo Fisher Scientific) equipped with a Gatan K3 detector and BioQuantum energy filter, using SerialEM. Grids were loaded such that the lamella pretilt axis aligns with the microscope stage tilt axis. Images were acquired at a pixel size of 3.370 or 3.425 Å/pixel at 1.5–4.0 µm defocus, with an electron dose of 2.0–2.5 e−/Å2 per image fractionated over 8–10 frames. A dose-symmetric tilt scheme was used with 2° increments starting from the lamella pretilt (±13°) and an effective tilt range of +56° to −56° using SerialEM. Data were collected with a 70-µm objective aperture and 20 eV slit width.",
      "labEquipment": "Titan Krios G3 (Thermo Fisher Scientific) with Gatan K3 detector and BioQuantum energy filter",
      "measurementTechnique": [
        {
          "@id": "obo:FBbi_00000256"
        }
      ],
      "name": "Cryo-electron tomography with defocus phase contrast"
