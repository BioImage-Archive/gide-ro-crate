    {
      "@id": "#22996f24-29b7-4f52-bb7a-061f1219c411",
      "@type": [
        "BioSample"
      ],
      "description": "Red snow alga collected from snow fields at Vallon Roche Noir, French Alps at 2318 m altitude. Cells are unicellular cysts with diameter of 18.8 ± 3.9 µm containing lipid droplets and carotenoids.",
      "name": "Sanguina nivaloides snow algae",
      "taxonomicRange": {
        "@id": "obo:NCBITaxon_2578421"
      }
    },
    {
      "@id": "#a51f22e1-a5c0-42bb-a04c-4f9897f64b08",
      "@type": [
        "LabProtocol"
      ],
      "description": "Focused ion beam (FIB) tomography was performed with either a Zeiss NVision 40 or a Zeiss CrossBeam 550 microscope, both equipped with Fibics Atlas 3D software for tomography. The sample was abraded slice by slice with the Ga+ ion beam (generally with a current of 700 nA at 30 kV). Each freshly exposed surface was imaged by scanning electron microscopy (SEM) at 1.5 kV and with a current of ~1 nA using the in-column EsB backscatter detector. For each slice, a thickness of 8 nm for the cryo-fixed cells and 10 nm for the chemically fixed cells was removed, and the SEM images were recorded with a pixel size of 8 nm and 10 nm respectively, providing an isotropic voxel size of 8×8×8 nm3 and 10×10×10 nm3.",
      "labEquipment": "Zeiss NVision 40 or Zeiss CrossBeam 550 microscope with Fibics Atlas 3D software",
      "measurementTechnique": [
        {
          "@id": "obo:FBbi_00050000"
        }
      ],
      "name": "FIB-SEM Imaging"
    },
    {
      "@id": "obo:FBbi_00050000",
      "@type": [
        "DefinedTerm"
      ],
      "name": "focussed ion beam scanning electron microscopy (FIB-SEM)"
    },
